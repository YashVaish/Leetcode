class Solution {
public:
    bool isPalindrome(int x) {
        if(x<0)
            return false;
        else
        {
            int rev=0,org=x;
            while(x>0)
            {
                if(rev>INT_MAX/10)
                    return false;
                rev=rev*10+x%10;
                x=x/10;
            }
            if(rev==org)
                return true;
            else
                return false;
        }
    }
};
